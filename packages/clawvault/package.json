{
  "name": "@clawsentinel/clawvault",
  "version": "0.7.0",
  "description": "Encrypted credential vault for ClawSentinel â€” AES-256-GCM + OS keychain",
  "license": "Elastic-2.0",
  "author": "hshdevhub",
  "homepage": "https://clawsentinel.sh",
  "repository": {
    "type": "git",
    "url": "https://github.com/hshdevhub/clawsentinel.git",
    "directory": "packages/clawvault"
  },
  "keywords": [
    "clawsentinel",
    "openclaw",
    "ai-security",
    "credential-vault",
    "keychain",
    "aes-256"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
